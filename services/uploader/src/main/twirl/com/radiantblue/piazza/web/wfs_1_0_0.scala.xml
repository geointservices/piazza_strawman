  <Service>
    <Name>My GeoServer WFS</Name>
    <Title>My GeoServer WFS</Title>
    <Abstract>
This is a description of your Web Feature Server.

The GeoServer is a full transactional Web Feature Server, you may wish to limit
GeoServer to a Basic service level to prevent modificaiton of your geographic
data.
     </Abstract>
    <Keywords>WFS, WMS, GEOSERVER</Keywords>
    <OnlineResource>http://192.168.23.13:8080/geoserver/wfs</OnlineResource>
    <Fees>NONE</Fees>
    <AccessConstraints>NONE</AccessConstraints>
  </Service>
  <Capability>
    <Request>
      <GetCapabilities>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=GetCapabilities"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </GetCapabilities>
      <DescribeFeatureType>
        <SchemaDescriptionLanguage>
          <XMLSCHEMA/>
        </SchemaDescriptionLanguage>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=DescribeFeatureType"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </DescribeFeatureType>
      <GetFeature>
        <ResultFormat>
          <KML/>
          <GML2/>
          <GML3/>
          <SHAPE-ZIP/>
          <CSV/>
          <JSON/>
        </ResultFormat>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=GetFeature"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </GetFeature>
      <Transaction>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=Transaction"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </Transaction>
      <LockFeature>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=LockFeature"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </LockFeature>
      <GetFeatureWithLock>
        <ResultFormat>
          <GML2/>
        </ResultFormat>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://192.168.23.13:8080/geoserver/wfs?request=GetFeatureWithLock"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://192.168.23.13:8080/geoserver/wfs"/>
          </HTTP>
        </DCPType>
      </GetFeatureWithLock>
    </Request>
  </Capability>
  <FeatureTypeList>
    <Operations>
      <Query/>
      <Insert/>
      <Update/>
      <Delete/>
      <Lock/>
    </Operations>
    @for((md, geo) <- layers) {
    <FeatureType>
      <Name>piazza:@md.getLocator</Name>
      <Title>@md.getName</Title>
      <Abstract/>
      <Keywords>features, states.zip</Keywords>
      <SRS>@geo.getCrsCode</SRS>
      <LatLongBoundingBox minx="@geo.getLatitudeLongitudeBoundingBox.getMinX" miny="@geo.getLatitudeLongitudeBoundingBox.getMinY" maxx="@geo.getLatitudeLongitudeBoundingBox.getMaxX" maxy="@geo.getLatitudeLongitudeBoundingBox.getMaxY"/>
    </FeatureType>
    }
